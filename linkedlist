#include<iostream>
using namespace std;
int main(){
    struct node{
int data;
Node *next;
};
int main(){
    Node * head=new Node;
    Node * second=new Node;
    Node * third=new Node;
    head->data=10;
    head->next=second;
    
    second->data=20;
    second->next=third;
    
    third->data=30;
    third->next=null;
    
    Node*temp=head;
    while(temp!=NULL){
        cout<<temp->data<<"";
        temp=temp->next;
    }
    return 0;
}
